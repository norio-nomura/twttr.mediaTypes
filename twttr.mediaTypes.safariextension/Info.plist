<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE plist PUBLIC "-//Apple//DTD PLIST 1.0//EN" "http://www.apple.com/DTDs/PropertyList-1.0.dtd">
<plist version="1.0">
<dict>
	<key>Author</key>
	<string>Norio Nomura</string>
	<key>CFBundleDisplayName</key>
	<string>twttr.mediaTypes</string>
	<key>CFBundleIdentifier</key>
	<string>com.github.norio-nomura.twttr.mediaTypes</string>
	<key>CFBundleInfoDictionaryVersion</key>
	<string>6.0</string>
	<key>CFBundleShortVersionString</key>
	<string>1.4</string>
	<key>CFBundleVersion</key>
	<string>1.4</string>
	<key>Chrome</key>
	<dict>
		<key>Global Page</key>
		<string>global.html</string>
	</dict>
	<key>Content</key>
	<dict>
		<key>Scripts</key>
		<dict>
			<key>End</key>
			<array>
				<string>injectHTMLLinkElement.js</string>
				<string>twttr.media.types.instagram.listener.js</string>
				<string>twttr.media.types.posterous.listener.js</string>
				<string>twttr.media.types.slideshare.listener.js</string>
			</array>
			<key>Start</key>
			<array>
				<string>injectHTMLScriptElement.js</string>
			</array>
		</dict>
		<key>Whitelist</key>
		<array>
			<string>http://twitter.com/</string>
			<string>https://twitter.com/</string>
			<string>http://twitter.com/#*</string>
			<string>https://twitter.com/#*</string>
		</array>
	</dict>
	<key>Description</key>
	<string>Show preview image from some sites on #NewTwitter.</string>
	<key>ExtensionInfoDictionaryVersion</key>
	<string>1.0</string>
	<key>Permissions</key>
	<dict>
		<key>Website Access</key>
		<dict>
			<key>Allowed Domains</key>
			<array>
				<string>twitter.com</string>
				<string>instagr.am</string>
				<string>posterous.com</string>
				<string>www.slideshare.net</string>
			</array>
			<key>Include Secure Pages</key>
			<true/>
			<key>Level</key>
			<string>Some</string>
		</dict>
	</dict>
	<key>Update Manifest URL</key>
	<string>http://norio-nomura.github.com/twttr.mediaTypes/Manifest.plist</string>
	<key>Website</key>
	<string>http://norio-nomura.github.com/twttr.mediaTypes/</string>
</dict>
</plist>
