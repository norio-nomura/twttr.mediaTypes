{
    "name":"twttr.mediaTypes",
    "version":"1.15",
    "description":"Show preview image from some sites on #NewTwitter.",
    "icons":{
        "16":"Icon-16.png",
        "48":"Icon-48.png",
        "128":"Icon-128.png"
    },
    "background_page":"global.html",
    "content_scripts":[
        {
            "matches":[
                "http://twitter.com/",
                "https://twitter.com/",
                "http://twitter.com/#*",
                "https://twitter.com/#*"
            ],
            "js":[
                "injectHTMLScriptElement.js"
            ],
            "run_at":"document_start"
        },
        {
            "matches":[
                "http://twitter.com/",
                "https://twitter.com/",
                "http://twitter.com/#*",
                "https://twitter.com/#*"
            ],
            "js":[
                "injectHTMLLinkElement.js",
                "twttr.media.types.cameraplus.listener.js",
                "twttr.media.types.cloudapp.listener.js",
                "twttr.media.types.posterous.listener.js",
                "twttr.media.types.twitlonger.listener.js"
            ],
            "run_at":"document_end"
        }
    ],
    "options_page": "options.html",
    "permissions":[
        "http://campl.us/*",
        "http://www.cl.ly/*",
        "http://posterous.com/api/getpost",
        "http://www.twitlonger.com/api_read/*"
    ]
}